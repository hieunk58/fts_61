<h4>
  <%= t "views.exams.created_date" %>
  <%= @examination.created_at %>
</h4>
<div class="row">
  <div class="col-md-6 col-md-offset-1">
    <%= form_for @examination do |f| %>
       <ol>
          <%= f.fields_for :results do |result| %>
            <%= render "question", f: result %>
          <% end %>
      </ol>
    <% end %>
    <% if @examination.unchecked? %>
      <%= form_tag admin_examination_path(@examination), method: :put do %>
        <%= submit_tag :checked, class: " btn btn-md btn-success" %>
      <% end %>
    <% end %>
  </div>
</div>
